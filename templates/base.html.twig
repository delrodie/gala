<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
            <link rel="stylesheet" href="{{ asset('css/style.css') }}">
            <link rel="stylesheet" href="{{ asset('css/responsive.css') }}">
        {% endblock %}
    </head>
    <body>
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <a class="navbar-brand" href="{{ path('homepage') }}">
            <img src="{{ asset('images/rotary.png') }}" class="logo-rotary" width="350" alt="logo rotary">
        </a>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">

            <div class="col-md-2"></div>
            <div class="col-md-8 banner-title">Le Rotary des innovateurs</div>
            <div class="col-md-1"></div>
        </div>
        <img src="{{ asset('images/logo-district.png') }}" class="logo-district" alt="logo du district" width="250">
    </nav>
        {% block body %}{% endblock %}
        <section class="footer">
            <div class="container">
                <div class="row copyright">
                    <div class="col-8">
                        &copy; Copyright 2020 <a href="http://dreammaker-ci.com" target="_blank">DREAM MAKER</a> | Tous droits réservés.
                    </div>
                    <div class="col-4 text-right"><a href="{{ path('backend') }}" target="_blank">Backoffice</a></div>
                </div>
            </div>
        </section>
        {% block javascripts %}
            <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
            <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
        {% endblock %}
    </body>
</html>
